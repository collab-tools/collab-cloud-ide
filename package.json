{
  "name": "collab-cloud-ide",
  "version": "0.0.1",
  "description": "Web based IDE that provides collaboration opportunities",
  "main": "server.compiled.js",
  "scripts": {
    "start": "node server.compiled.js",
    "compile": "babel -d dist/ app/ && babel server.js -o server.compiled.js",
    "dev": "nodemon server.js -L --watch server.js --watch app --watch config --exec babel-node"
  },
  "repository": {
    "type": "git",
    "url": "git+https://hooitong@github.com/collab-tools/collab-cloud-ide.git"
  },
  "author": "Yeap Hooi Tong",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/collab-tools/collab-cloud-ide/issues"
  },
  "homepage": "https://github.com/collab-tools/collab-cloud-ide#readme",
  "private": true,
  "engines": {
    "node": ">=4"
  },
  "devDependencies": {
    "babel-core": "^6.4.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-register": "^6.5.2",
    "del": "^2.2.0",
    "gulp": "^3.9.0",
    "gulp-autoprefixer": "^3.0.1",
    "gulp-babel": "^6.1.1",
    "gulp-cache": "^0.4.2",
    "gulp-cssnano": "^2.0.0",
    "gulp-eslint": "^3.0.0",
    "gulp-htmlmin": "^3.0.0",
    "gulp-if": "^2.0.0",
    "gulp-imagemin": "^3.0.1",
    "gulp-load-plugins": "^1.2.4",
    "gulp-plumber": "^1.0.1",
    "gulp-size": "^2.1.0",
    "gulp-sourcemaps": "^1.5.0",
    "gulp-uglify": "^2.0.0",
    "gulp-useref": "^3.0.0",
    "main-bower-files": "^2.5.0",
    "run-sequence": "^1.2.2"
  },
  "eslintConfig": {
    "env": {
      "es6": true,
      "node": true,
      "browser": true
    },
    "rules": {
      "quotes": [
        2,
        "single"
      ]
    }
  },
  "dependencies": {
    "body-parser": "^1.15.2",
    "compression": "^1.6.2",
    "config": "^1.21.0",
    "express": "^4.14.0",
    "jsonwebtoken": "^7.1.9",
    "redis": "^2.6.2"
  }
}
